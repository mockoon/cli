{
  "name": "@mockoon/cli",
  "description": "Mockoon's CLI. Deploy your mock APIs anywhere.",
  "version": "1.0.0",
  "author": {
    "name": "Guillaume Monnet",
    "email": "hi@255kb.dev",
    "url": "https://255kb.dev/"
  },
  "bin": {
    "mockoon-cli": "./bin/run"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint -c .eslintrc.js --ext .js,.ts .",
    "format": "prettier {src,test}/**/*.{ts,js,scss,html} --check",
    "test": "ts-mocha -p ./tsconfig-build.json --timeout 10000 --forbid-only --file test/libs/setup.ts test/**/*.spec.ts",
    "build": "rm -rf dist && tsc -p tsconfig-build.json && oclif-dev manifest"
  },
  "main": "./dist/index.js",
  "files": [
    "/dist",
    "/bin",
    "/oclif.manifest.json"
  ],
  "oclif": {
    "commands": "./dist/commands",
    "hooks": {
      "init": "./dist/hooks/init"
    },
    "bin": "mockoon-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 80,
    "trailingComma": "none"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockoon/cli.git"
  },
  "bugs": {
    "url": "https://github.com/mockoon/cli/issues"
  },
  "homepage": "https://github.com/mockoon/cli#readme",
  "funding": "https://mockoon.com/sponsor-us/",
  "dependencies": {
    "@mockoon/commons": "2.1.3",
    "@mockoon/commons-server": "2.1.1",
    "@oclif/command": "1.8.0",
    "@oclif/config": "1.17.0",
    "@oclif/plugin-help": "3.2.2",
    "axios": "0.21.1",
    "cli-ux": "5.5.1",
    "inquirer": "8.0.0",
    "is-port-reachable": "3.0.0",
    "jsonfile": "6.1.0",
    "minimist": "1.2.5",
    "mkdirp": "1.0.4",
    "mustache": "4.2.0",
    "pm2": "4.5.6",
    "pretty-bytes": "5.6.0",
    "tslib": "2.2.0",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.0",
    "@oclif/test": "1.2.8",
    "@types/chai": "4.2.18",
    "@types/inquirer": "7.3.1",
    "@types/jsonfile": "6.0.0",
    "@types/minimist": "1.2.1",
    "@types/mkdirp": "1.0.1",
    "@types/mocha": "8.2.2",
    "@types/mustache": "4.1.1",
    "@types/node": "15.0.2",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "chai": "4.3.4",
    "eslint": "7.26.0",
    "eslint-config-oclif": "3.1.0",
    "eslint-config-oclif-typescript": "0.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-jsdoc": "33.1.1",
    "globby": "11.0.3",
    "mocha": "8.4.0",
    "prettier": "2.3.0",
    "prettier-plugin-organize-imports": "2.0.0",
    "ts-mocha": "8.0.0",
    "typescript": "4.2.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "types": "dist/index.d.ts"
}
